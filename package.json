{
  "name": "smarz-lab",
  "version": "2.0.0",
  "description": "smarz-lab.com theme",
  "author": "xlthlx",
  "keywords": [
	"WordPress",
	"Theme"
  ],
  "homepage": "https://smarz-lab.com/",
  "repository": {
	"type": "git",
	"url": "git+https://github.com/xlthlx/smarz-lab"
  },
  "scripts": {
	"minify:css": "cleancss -o assets/css/main.min.css assets/css/main.css",
	"watch:css": "onchange 'src/css/*.css' -- npm run minify:css",
	"watch": "run-p watch:*",
	"build": "npm run minify:css"
  },
  "devDependencies": {
	"clean-css-cli": "^5.6.1",
	"npm-run-all": "^4.1.5",
	"onchange": "^7.1.0"
  }
}
