{
  "name": "xlthlx/boot",
  "license": "MIT",
  "description": "Starter Theme based on Timber and Bootstrap 5.",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
    {
      "email": "xlthlx@gmail.com",
      "name": "xlthlx"
    }
  ],
  "require": {
    "timber/timber": "1.*",
    "johnbillion/extended-cpts": "^4.5",
    "cmb2/cmb2": "^2.9",
    "composer/installers": "^1.11",
    "benmorel/ebay-sdk-php": "^19.0"
  },
  "extra": {
    "installer-paths": {
      "inc/{$vendor}/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "require-dev": {
    "dxw/phar-install": "^1.1"
  },
  "scripts": {
    "post-update-cmd": "vendor/bin/phar-install"
  }
}
